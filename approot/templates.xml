    <message id="pp.template">
      <table class="myTable">
	<tr>
		<th colspan="5">PROCESS PLAN SHEET : ${process_code}</th>
		<th>PAGE : 1 / 1 <!--${page_no} --> </th>
	</tr>
	<tr>
		<td colspan="5"><b>Units : </b> Degrees, mm, kN, sec</td>
		<td><b>MODULES :</b> ${module_no}</td>
	</tr>
	<tr>
		<td colspan="3"><b>ENGINEER :</b> ${planner_name}</td>
		<td colspan="2"><b>CHECKED BY :</b> ${moderator}</td>
		<td colspan="1"><b>DATE CREATED : </b>${creation_date}</td>
	</tr>
	<tr >
		<td colspan="3"><b>PART No :</b> ${part_no}</td>		
	  	<td colspan="1"><b>PART NAME :</b> ${part_name}</td>
	  	<td colspan="1"><b>MATERIAL :</b> ${material_name}</td>	  
		<td colspan="1"><b>QUANTITY :</b> ${quantity}</td>	 	
	</tr>
	<tr>
	  	<!-- <td height="80"><b>STOCK SIZE :</b> ${stock_size}</td> -->
